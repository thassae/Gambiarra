{
    "manifest_version": 2,
    "name": "Gambiarra",
    "description": "Aplicativo para auxiliar os alunos de graduação da UERJ",
    "version": "0.1",
    "permissions": [
        "https://*.uerj.br/*",
        "http://*.uerj.br/*",
        "tabs", 
        "webRequest", 
        "webRequestBlocking", 
        "\u003Call_urls\u003E"
    ],
    "browser_action": {
        "default_icon": "icon.png",
        "default_popup": "popup.html"
    },
    "background": {
        "scripts": ["jquery.min.js", "background.js"]
    }
}